    display.drawLine(121 - Distancef + I, 54, 125 - Distancef + I, 63, WHITE);
    //structure
    display.fillRect(Jf + I, 26, Kf + I, 45, SSD1306_WHITE);
    display.display ();
    display.clearDisplay();

  }
}




